# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'talkTek' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

# Pods for talkTek

# Firebase
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'

# Facebook
  pod 'Bolts'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'

# Google
  pod 'GoogleSignIn'

# ObjectMapper
pod 'ObjectMapper', '~> 2.2'

# Kingfisher
pod 'Kingfisher', '~> 4.0'

# Sound
pod 'SwiftySound'


  target 'talkTekTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'talkTekUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
